{
    "name": "meng-tian/soap-http-psr",
    "description": "A SOAP client build on top of PSR7, PSR-17 and PSR-18.",
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "~1.0",
        "psr/http-factory": "~1.0",
        "psr/http-client": "~1.0",
        "meng-tian/soap-http-binding": "~0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "guzzlehttp/psr7": "~2.0",
        "guzzlehttp/guzzle": "~7.4"
    },
    "autoload": {
        "psr-4": {"Meng\\Soap\\PSR\\": "src/"}
    }
}
